    "react-dom": "17.0.1",
    "sass": "1.32.6",
    "tailwindcss": "2.0.3"
  },
  "devDependencies": {
    "@types/node": "14.14.25",
    "@types/react": "17.0.1",
    "hygen": "6.0.4",
    "standard-version": "9.1.0",
    "stylelint": "13.9.0",
    "stylelint-config-standard": "20.0.0",
    "typescript": "4.1.3"
  }
